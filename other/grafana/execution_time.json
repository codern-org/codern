      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "from(bucket: \"gateway\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) =>\n    r._measurement == \"httpRequest\"\n    and r._field == \"executionTime\"\n  )\n  |> aggregateWindow(\n    every: v.windowPeriod,\n    fn: (tables=<-, column) => tables\n        |> quantile(q: 0.99),\n  )\n  |> map(fn: (r) => ({ _value: r._value, _time: r._time, _field: \"P99 - ${r.path}\" }))",
          "refId": "P99"
        },
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "hide": false,
          "query": "from(bucket: \"gateway\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) =>\n    r._measurement == \"httpRequest\"\n    and r._field == \"executionTime\"\n  )\n  |> aggregateWindow(\n    every: v.windowPeriod,\n    fn: (tables=<-, column) => tables\n        |> quantile(q: 0.50),\n  )\n  |> map(fn: (r) => ({ _value: r._value, _time: r._time, _field: \"P50 - ${r.path}\" }))",
          "refId": "P50"
        }
      ],
